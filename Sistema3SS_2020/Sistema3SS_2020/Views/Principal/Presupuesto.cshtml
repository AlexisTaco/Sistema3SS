    <!-- Favicons -->

    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.6.2/jquery.min.js" type="text/javascript"></script>
    <link rel="apple-touch-icon" href="/docs/4.5/assets/img/favicons/apple-touch-icon.png" sizes="180x180">
    <link rel="icon" href="/docs/4.5/assets/img/favicons/favicon-32x32.png" sizes="32x32" type="image/png">
    <link rel="icon" href="/docs/4.5/assets/img/favicons/favicon-16x16.png" sizes="16x16" type="image/png">
    <link rel="manifest" href="/docs/4.5/assets/img/favicons/manifest.json">
    <link rel="mask-icon" href="/docs/4.5/assets/img/favicons/safari-pinned-tab.svg" color="#563d7c">
    <link rel="icon" href="/docs/4.5/assets/img/favicons/favicon.ico">
    <meta name="msapplication-config" content="/docs/4.5/assets/img/favicons/browserconfig.xml">
    <meta name="theme-color" content="#563d7c">


    <style>
        .bd-placeholder-img {
            font-size: 1.125rem;
            text-anchor: middle;
            -webkit-user-select: none;
            -moz-user-select: none;
            -ms-user-select: none;
            user-select: none;
        }
    </style>
    <!-- Custom styles for this template -->
    <script src="~/js/Presupuesto.js"></script>
    <!-- Custom styles for this template -->
    <link href="form-validation.css" rel="stylesheet">
</head>
<body class="bg-light">
    <br />
    <br />
    <br />
    <br />
    <br />
    <div class="container">
        <div class="row mb-4">
            <div class="col-4 mb-4">
                <div class="col-10 mb-0">
                    <label for="country">Presupuesto</label>
                    <select class="custom-select d-block w-100" id="country" required="">
                        <option value="">Tipo...</option>
                        <option>Economico</option>
                        <option>Produccion</option>
                    </select>
                    <div class="invalid-feedback">
                        Por favor de selccionar un elemnto valido
                    </div>
                </div>
            </div>
            <div class="col-4 ">
                <div class="col-10 mb-0">
                    <label for="state">Granja</label>
                    <select onchange="jsFuncionNumeroEstanques();" name="SelectGranja" class="custom-select d-block w-100" id="SelectGranja" runat="server">
                        <option value="">Nombre...</option>
                        @foreach (var granja in Model)
                        {
                            <option value="@granja.numero_estanques">@granja.nombre</option>
                        }
                    </select>
                    <div class="invalid-feedback">
                        Por favor de selccionar un elemnto valido
                    </div>
                </div>
            </div>
            <div class="col-4 ">
                <div class="col-10 mb-0">
                    <label for="state">Concepción</label>
                    <select onchange="jsFuncion();" name="SelectConcepcion" class="custom-select d-block w-100" id="SelectConcepcion">
                        <option value="">Selecionar...</option>
                        <option>Granja en general</option>
                        <option>Estanque unico</option>
                        <option value="1">Multiples estanques</option>
                    </select>
                    <div class="invalid-feedback">
                        Por favor de selccionar un elemnto valido
                    </div>
                </div>
            </div>
            <div class="col-4">
                <div class="col-10 mb-0">
                    <label for="state">Selecion estanques</label>
                    <select disabled name="Selectestanquest" class="custom-select d-block w-100" id="Selectestanques">
                        <option value="">Numero...</option>
                    </select>
                    <div class="invalid-feedback">
                        Por favor de selccionar un elemnto valido
                    </div>
                </div>
            </div>
        </div>
        <div class="row mb-4">
            <div class="col-4 mb-4">
                <div class="col-10 mb-0">
                    <label for="country">Costo</label>
                    <select class="custom-select d-block w-100" id="country" required="">
                        <option value="">Nombre...</option>
                        <option>Papelaeria</option>
                        <option>baños</option>
                    </select>
                    <div class="invalid-feedback">
                        Por favor de selccionar un elemnto valido
                    </div>
                </div>
            </div>
            <div class="col-4 mb-4">
                <div class="col-10 mb-0">
                    <label for="country">Concepto</label>
                    <select class="custom-select d-block w-100" id="country" required="">
                        <option value="">contenido...</option>
                        <option>Plumas</option>
                        <option>Jabon</option>
                    </select>
                    <div class="invalid-feedback">
                        Por favor de selccionar un elemnto valido
                    </div>
                </div>
            </div>
        </div>
    </div>
    <hr class="mb-4">
</body>
</html>